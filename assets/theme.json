      "normal_border": "#f2f489"
    },
    "misc": {
      "shape": "rectangle",
      "border_width": 0,
      "shadow_width": 0
    }
  },

  "@sub_panel": {
    "colours": {
      "dark_bg": "#f2f489",
      "normal_border": "#f2f489"
    },
